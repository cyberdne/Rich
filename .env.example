# Bot Configuration
BOT_TOKEN=YOUR_BOT_TOKEN_HERE
ADMIN_IDS=YOUR_USER_ID_HERE,OTHER_ADMIN_IDS
LOG_CHANNEL_ID=YOUR_LOG_CHANNEL_ID_HERE

# AI Services (Optional)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Environment
DEBUG_MODE=false
NODE_ENV=production

# Auto-fix options
AUTO_FIX_ENABLED=false
